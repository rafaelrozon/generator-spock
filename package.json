{
    "name": "generator-spock",
    "version": "1.0.1",
    "description": "React/Redux Yeoman Generators",
    "license": "MIT",
    "engines": {
        "npm": ">= 4.0.0"
    },
    "repository": "rafaelrozon/generator-spock",
    "author": "Rafael Rozon",
    "keywords": [
        "react",
        "redux",
        "reselect",
        "generator",
        "yeoman",
        "yo",
        "spock",
        "yeoman-generator"
    ],
    "main": "generators/index.js",
    "files": [
        "generators"
    ],
    "scripts": {
        "clean": "rm -rf node_modules",
        "format:pkg": "format-package -w",
        "pretest": "npm run lint",
        "test": "jest",
        "lint": "eslint .",
        "lint-fix": "eslint . --fix"
    },
    "lint-staged": {
        "*.js": [
            "eslint --fix",
            "git add"
        ],
        "*.json": [
            "prettier --write",
            "git add"
        ]
    },
    "eslintConfig": {
        "env": {
            "jest": true,
            "node": true
        },
        "extends": [
            "xo",
            "prettier"
        ],
        "plugins": [
            "prettier"
        ],
        "rules": {
            "prettier/prettier": "error"
        }
    },
    "jest": {
        "testEnvironment": "node"
    },
    "dependencies": {
        "chalk": "^2.1.0",
        "change-case": "^3.0.2",
        "yeoman-generator": "^2.0.1",
        "yosay": "^2.0.1"
    },
    "devDependencies": {
        "eslint": "^5.9.0",
        "eslint-config-prettier": "^3.3.0",
        "eslint-config-xo": "^0.25.0",
        "eslint-plugin-prettier": "^3.0.0",
        "format-package": "^5.1.0",
        "husky": "^1.2.0",
        "jest": "^23.5.0",
        "lint-staged": "^8.1.0",
        "prettier": "^1.15.2",
        "yeoman-assert": "^3.1.0",
        "yeoman-test": "^1.7.0"
    }
}
